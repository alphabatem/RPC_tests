                },
                "requestsPerSec": {
                    "type": "number"
                },
                "successCount": {
                    "type": "integer"
                },
                "successRate": {
                    "type": "number"
                },
                "totalRequests": {
                    "type": "integer"
                }
            }
        },
        "time.Duration": {
            "type": "integer",
