                },
                "duration": {
                    "type": "integer"
                },
                "enabled": {
                    "type": "boolean"
                },
                "limit": {
                    "type": "integer"
                }
            }
        },
        "cmd.OverallResult": {
            "type": "object",
            "properties": {
                "methodResults": {
                    "type": "array",
